YUI.add("cookie-banner-view",function(e,i){require("hermes-core/flog")(i);e.FlickrView.create(this.name,e.FlickrView,[],{langBundles:this.details.langBundles,initializer:function(e){this.params=e},loadState:function(){return e.Promise.resolve()},buildContainer:function(){var e="<p>"+this.intlMessage({intlName:"privacy.COOKIE_BANNER_TEXT",url:"/help/cookies"})+"</p>";return this.setContainerWithTemplate("basic-banner",{message:e,additionalClasses:"foot-banner-style"}),this},activate:function(){var e=this.get("container");return this.registerEventHandler(e.one(".button.dismiss").on("click",this.dismissBanner.bind(this))),this.fire("activated"),this},dismissBanner:function(i){var s=new Date;s.setTime(s.getTime()+31536e6),this.appContext.cookieCatcher.setCookieIfAllowed(e.config.flickr.banners.dismiss_cookie_banner_name,1,{expires:s,path:"/"}),this.fire("dismiss")}})},"@VERSION@",{requires:["flickr-view","hermes-template-basic-banner","user-limits-helper","person-models","person-profile-models"],langBundles:["privacy"]});YUI.add("hermes-lang-privacy_ko-kr",function(r,a){r.Intl.add("hermes/privacy","ko-KR",{OATH_DATA_USAGE:["Flickr 및 Yahoo는 현재 Verizon 회사와 합병되어 “Oath”라는 기업으로 새롭게 선보입니다. 2017년 9월 15일부터 새로운 회사 내에서 일부 사용자 정보를 공유할 계획입니다."],SM:["흥미진진한 소식: Flickr가 SmugMug에 합류합니다!"],COOKIE_BANNER_TEXT:['이 사이트는 귀하의 경험을 개선하고 귀하의 관심사와 더 관련이 있는 콘텐츠를 표시하기 위해 쿠키를 사용합니다. 이 사이트를 사용하면 <a target="_blank" href="',"${url}",'">쿠키 정책</a>에 설명된 대로 Flickr 및 파트너의 쿠키 사용에 동의하는 것입니다.']})},"@VERSION@",{requires:["intl"]});